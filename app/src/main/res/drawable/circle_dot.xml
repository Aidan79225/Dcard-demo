<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/mainLayer">
        <shape
            android:shape="oval"
            android:tint="@color/bg_dark_blue">
            <corners android:radius="10dp" />
            <solid android:color="@color/bg_dark_gray" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <corners android:radius="10dp" />
            <stroke android:color="@color/text_dark_gray" android:width="2dp"/>
        </shape>
    </item>
</layer-list>
